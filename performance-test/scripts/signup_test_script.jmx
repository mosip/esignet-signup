              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import java.time.Instant
import java.time.ZoneId
import java.time.ZonedDateTime
import java.time.format.DateTimeFormatter

// Get current time in milliseconds as a long
long instant = System.currentTimeMillis()+60000;

// Convert to Instant
Instant timestamp = Instant.ofEpochMilli(instant)

// Convert to UTC time zone
ZonedDateTime utcTime = timestamp.atZone(ZoneId.of(&quot;UTC&quot;))

// Format output with microseconds
DateTimeFormatter formatter = DateTimeFormatter.ofPattern(&quot;yyyy-MM-dd HH:mm:ss.SSSSSS&quot;)
String formattedUtcTime = utcTime.format(formatter)

// Print to JMeter log
log.info(&quot;Test Start Time in UTC: &quot; + formattedUtcTime)
vars.put(&quot;formattedUtcTime&quot;,formattedUtcTime);
</stringProp>
