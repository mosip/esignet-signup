                  <stringProp name="cacheKey">false</stringProp>
                  <stringProp name="script">def fetchStartTimestamp=vars.get(&quot;timeStamp&quot;);
def success=vars.get(&quot;pendingStatusCheck&quot;);
def currentTimestamp=&quot;${__time(,)}&quot;;
long startTimestamp=fetchStartTimestamp.toLong();
long endTimestamp=currentTimestamp.toLong();
int timeConsumed=endTimestamp-startTimestamp;
//log.info(success);
if(timeConsumed &gt;= 200000)
{
def timeoutCheck=&quot;DONE&quot;;
vars.put(&quot;check&quot;,timeoutCheck);
}
if(timeConsumed &lt; 200000 &amp;&amp; success.equals(&quot;PENDING&quot;))
{
prev.setIgnore();
}

