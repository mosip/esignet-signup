            <stringProp name="script">import java.io.*;

// Creating base64-urljsonencoded
def fullNameVal=vars.get(&quot;fullName&quot;);
def langVal=vars.get(&quot;signUpRegisterLanguage&quot;);
def fullPayload=&quot;{\&quot;fullName\&quot;:[{\&quot;language\&quot;:\&quot;${langVal}\&quot;,\&quot;value\&quot;:\&quot;${fullNameVal}\&quot;}]}&quot;
def encodedBytes = fullpayload.getBytes(&quot;UTF-8&quot;)
def encodedString = encodedBytes.encodeBase64().toString()

//adding the identifier number
def identifierPrefix=vars.get(&quot;identifierPrefix&quot;);
def identifier=vars.get(&quot;identifier&quot;);
def phonenum=identifierprefix+identifier;

//defining bin folder of JMeter path
def currentDir = System.getProperty(&quot;user.dir&quot;);
//def binDir = new File(currentDir);

def runtimeFilesDir = new File(currentDir, &quot;bin/runtime-files&quot;);

//saving to the file
File f = new File(runtimeFilesDir, &quot;testdata_resetpassword.txt&quot;);
FileWriter fw=new FileWriter(f,true);
BufferedWriter bw=new BufferedWriter(fw);
bw.write(&quot;${phonenum},${encodedString}&quot;);
bw.write(System.getProperty(&quot;line.separator&quot;));
bw.close();
fw.close();</stringProp>
