{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "./src/assets/*"
            ],
            "~contexts/*": [
                "./src/contexts/*"
            ],
            "~constants/*": [
                "./src/constants/*"
            ],
            "~components/*": [
                "./src/components/*"
            ],
            "~layouts/*": [
                "./src/layouts/*"
            ],
            "~templates/*": [
                "./src/templates/*"
            ],
            "~features/*": [
                "./src/features/*"
            ],
            "~hooks/*": [
                "./src/hooks/*"
            ],
            "~utils/*": [
                "./src/utils/*"
            ],
            "~pages/*": [
                "./src/pages/*"
            ],
            "~services/*": [
                "./src/services/*"
            ],
            "~theme/*": [
                "./src/theme/*"
            ],
            "~typings/*": [
                "./src/typings/*"
            ],
            "~/*": [
                "./src/*"
            ]
        }
    }
}
